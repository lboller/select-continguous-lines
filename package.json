{
  "name": "select-non-empty-lines",
  "displayName": "Select Non-Empty Lines",
  "description": "Select contiguous non-empty lines with various modes",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.selectContiguousNonEmptyLinesExpandForward",
    "onCommand:extension.selectContiguousNonEmptyLinesMoveForward",
    "onCommand:extension.selectContiguousNonEmptyLinesMoveBackward"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.selectContiguousNonEmptyLinesExpandForward",
        "title": "Select Contiguous Non-Empty Lines (Expand Forward)"
      },
      {
        "command": "extension.selectContiguousNonEmptyLinesMoveForward",
        "title": "Select Contiguous Non-Empty Lines (Move Forward)"
      },
      {
        "command": "extension.selectContiguousNonEmptyLinesMoveBackward",
        "title": "Select Contiguous Non-Empty Lines (Move Backward)"
      }
    ],
    "keybindings": [
      {
        "command": "extension.selectContiguousNonEmptyLinesExpandForward",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l"
      },
      {
        "command": "extension.selectContiguousNonEmptyLinesMoveForward",
        "key": "ctrl+shift+j",
        "mac": "cmd+shift+j"
      },
      {
        "command": "extension.selectContiguousNonEmptyLinesMoveBackward",
        "key": "ctrl+shift+k",
        "mac": "cmd+shift+k"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
